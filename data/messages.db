midi-qol-other-roll\"></div>\n      </div>\n    </div>\n\n\n\n    <button data-action=\"save\" data-ability=\"wis\">\n      Saving Throw DC 16 Wisdom\n    </button>\n\n\n\n    \n  </div>\n\n  <footer class=\"card-footer\">\n    <span>1st Level</span>\n    <span>V,S,M (A morsel of food.)</span>\n    <span>1 Action</span>\n    <span>1 Creature</span>\n    <span>30 Feet</span>\n    <span>24 Hours</span>\n  </footer>\n</div>","speaker":{"scene":"T3nibRUezkEJXIoX","token":"9Wl6cQok6DkY0w78","actor":"ctwX2uWzOcOp3BuO","alias":"Naughty"},"whisper":[],"blind":false,"emote":false,"flags":{"midi-qol":{"itemUuid":"Actor.ctwX2uWzOcOp3BuO.Item.ezTvB26vavkkGFgJ","actorUuid":"Actor.ctwX2uWzOcOp3BuO","sound":"","type":0,"itemId":"ezTvB26vavkkGFgJ","workflowId":"Actor.ctwX2uWzOcOp3BuO.Item.ezTvB26vavkkGFgJ"},"core":{"canPopout":true}},"_id":"P1IJ7e5cCoec1J8v"}
